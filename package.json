{
  "name": "so-cx",
  "displayName": "SOCx",
  "version": "1.0.5",
  "description": "Simple OSINT Centralized eXtension",
  "author": "Alessio Mobilia",
  "privacy_policy": "https://socx.alessiomobilia.com/privacy.html",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@heroicons/react": "^2.2.0",
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "^0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "stable": "^0.1.8",
    "svgo": "^3.3.2",
    "tippy.js": "^6.3.7",
    "xlsx-js-style": "^1.2.0"
  },
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.html",
    "./**/**/*.ts",
    "./**/**/*.tsx",
    "./**/**/*.html"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./src/*"
      ]
    },
    "baseUrl": "."
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.18",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "file:///*"
    ],
    "permissions": [
      "contextMenus",
      "storage",
      "notifications",
      "clipboardWrite",
      "file:///*"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "assets/*"
        ]
      }
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core"
    ]
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "content-security-policy-parser": "^0.6.0",
    "msgpackr": "^1.10.1",
    "svelte": "^4.2.19"
  }
}
